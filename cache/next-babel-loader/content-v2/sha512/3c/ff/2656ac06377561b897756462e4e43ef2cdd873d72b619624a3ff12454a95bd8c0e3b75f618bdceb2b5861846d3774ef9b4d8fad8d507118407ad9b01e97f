{"ast":null,"code":"import { clientsAboutUsScroll } from '../../../config/constants'; //add event listener to sidebar items on case pages\n\nexport const caseSidebarEventListener = (elementsArr, scrollFnDesk, scrollFnTablet) => {\n  Array.from(elementsArr).forEach(el => {\n    el.addEventListener('click', () => {\n      if (window.innerWidth < 1200) {\n        scrollFnTablet(Number(el.parentElement.value));\n      } else {\n        scrollFnDesk(Number(el.parentElement.value));\n      }\n    });\n  });\n}; //event listener to buttons \"Clients about us\" on pages\n\nexport const clientsAboutUsBtnEventListeners = scrollFn => {\n  const values = Object.values(clientsAboutUsScroll);\n  values.forEach(({\n    buttonScrollID,\n    slide\n  }) => {\n    const button = document.getElementById(buttonScrollID);\n\n    if (button && window.innerWidth > 1199) {\n      button.addEventListener('click', () => {\n        scrollFn(slide);\n      });\n    }\n  });\n};","map":{"version":3,"sources":["E:/corporate-website/components/Scroll/utils/eventListeners.js"],"names":["clientsAboutUsScroll","caseSidebarEventListener","elementsArr","scrollFnDesk","scrollFnTablet","Array","from","forEach","el","addEventListener","window","innerWidth","Number","parentElement","value","clientsAboutUsBtnEventListeners","scrollFn","values","Object","buttonScrollID","slide","button","document","getElementById"],"mappings":"AAAA,SAASA,oBAAT,QAAqC,2BAArC,C,CAEA;;AACA,OAAO,MAAMC,wBAAwB,GAAG,CAACC,WAAD,EAAcC,YAAd,EAA4BC,cAA5B,KAA+C;AACrFC,EAAAA,KAAK,CAACC,IAAN,CAAWJ,WAAX,EAAwBK,OAAxB,CAAiCC,EAAD,IAAQ;AACtCA,IAAAA,EAAE,CAACC,gBAAH,CAAoB,OAApB,EAA6B,MAAM;AACjC,UAAIC,MAAM,CAACC,UAAP,GAAoB,IAAxB,EAA8B;AAC5BP,QAAAA,cAAc,CAACQ,MAAM,CAACJ,EAAE,CAACK,aAAH,CAAiBC,KAAlB,CAAP,CAAd;AACD,OAFD,MAEO;AACLX,QAAAA,YAAY,CAACS,MAAM,CAACJ,EAAE,CAACK,aAAH,CAAiBC,KAAlB,CAAP,CAAZ;AACD;AACF,KAND;AAOD,GARD;AASD,CAVM,C,CAYP;;AACA,OAAO,MAAMC,+BAA+B,GAAIC,QAAD,IAAc;AAC3D,QAAMC,MAAM,GAAGC,MAAM,CAACD,MAAP,CAAcjB,oBAAd,CAAf;AACAiB,EAAAA,M