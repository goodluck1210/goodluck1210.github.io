Germany', 'Switzerland', 'Austria'];\r\n    if (GermanCountries.includes(geolocation.country) && deFlag !== 'off'){\r\n      localStorage.setItem('deFlag', 'off');\r\n      router.push(routing().root + '?lang=de');\r\n    } else{\r\n      localStorage.removeItem('deFlag');\r\n    }\r\n  }, []);\r\n\r\n  return (\r\n    <>\r\n      <Scroll\r\n        titles={[\r\n          t('makeYour') + ' EDTECH ' + t('companies'),\r\n          t('ourServices'),\r\n          t('clientsSay'),\r\n          t('homePageVideoTitle'),\r\n          t('home.reasons.title'),\r\n          t('caseStudy'),\r\n          t('CTATitle'),\r\n        ]}\r\n      >\r\n        <Head>\r\n          <title>{meta.homePageTitle}</title>\r\n          <meta name=\"description\" content={meta.homePageDescription} />\r\n        </Head>\r\n        <ScrollSection>\r\n          <Header width={width} />\r\n        </ScrollSection>\r\n        <ScrollSection>\r\n          <Services data={services} />\r\n        </ScrollSection>\r\n        <ScrollSection>\r\n          <Reviews\r\n            data={reviews}\r\n            id={clientsAboutUsScroll.home.reviewComponentID}\r\n            width={width}\r\n          />\r\n        </ScrollSection>\r\n        <ScrollSection>\r\n          <Video />\r\n        </ScrollSection>\r\n        <ScrollSection>\r\n          <Reasons data={reasons} />\r\n        </ScrollSection>\r\n        <ScrollSection>\r\n          <CaseStudy mainTopTitle={t('caseStudy')} />\r\n        </ScrollSection>\r\n        <ScrollSection>\r\n          <BusinessPains data={business} width={width} />\r\n        </ScrollSection>\r\n        <ScrollSection>\r\n          <CTA CTATitle={t('CTATitle')} />\r\n        </ScrollSection>\r\n      </Scroll>\r\n      <script\r\n        type=\"application/ld+json\"\r\n        dangerouslySetInnerHTML={{\r\n          __html: JSON.stringify(OrganizationShema()),\r\n        }}\r\n      />\r\n    </>\r\n  );\r\n}\r\n\r\nconst fetcher = async (url, init) => {\r\n  return fetch(url