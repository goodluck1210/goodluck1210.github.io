{"ast":null,"code":"import routing from '../../config/routing';\nimport { SITE_DOMAIN, MAIN_CONTACT_EMAIL, INTERNATIONAL, COMPANY_STREET_ADDRESS, COMPANY_ADDRESS_LOCALITY, COMPANY_POSTAL_CODE } from '../../config/constants';\nexport var breadcrumbShema = function breadcrumbShema(blog, category, id, name) {\n  return {\n    '@context': 'https://schema.org',\n    '@type': 'BreadcrumbList',\n    itemListElement: [{\n      '@type': 'ListItem',\n      position: 1,\n      name: blog,\n      item: \"\".concat(SITE_DOMAIN).concat(routing().blog)\n    }, {\n      '@type': 'ListItem',\n      position: 2,\n      name: category,\n      item: \"\".concat(SITE_DOMAIN).concat(routing(category).blogCategory)\n    }, {\n      '@type': 'ListItem',\n      position: 3,\n      name: name,\n      item: \"\".concat(SITE_DOMAIN).concat(routing(id).article)\n    }]\n  };\n};\nexport var articleShema = function articleShema(_ref) {\n  var id = _ref.id,\n      title = _ref.title,\n      author = _ref.author,\n      img = _ref.img,\n      date = _ref.date,\n      logo = _ref.logo;\n  return {\n    '@context': 'https://schema.org',\n    '@type': 'NewsArticle',\n    mainEntityOfPage: {\n      '@type': 'WebPage',\n      id: \"\".concat(SITE_DOMAIN).concat(routing(id).article)\n    },\n    headline: title,\n    image: [img],\n    author: {\n      '@type': 'Person',\n      name: author\n    },\n    publisher: {\n      '@type': 'Organization',\n      name: 'WTT Solutions',\n      logo: {\n        '@type': 'ImageObject',\n        url: logo\n      }\n    },\n    datePublished: date,\n    dateModified: date\n  };\n};\nexport var ratingShema = function ratingShema(title, ratingValue, reviewCount) {\n  return {\n    '@context': 'http://schema.org',\n    '@type': 'CreativeWorkSeries',\n    name: title,\n    aggregateRating: {\n      '@type': 'AggregateRating',\n      ratingValue: ratingValue,\n      reviewCount: reviewCount\n    }\n  };\n};\nexport var FAQShema = function FAQShema(questions) {\n  var mainEntity = questions.map(function (_ref2) {\n    var question = _ref2.question,\n        answer = _ref2.answer;\n    return {\n      '@type': 'Question',\n      name: question,\n      acceptedAnswer: {\n        '@type': 'Answer',\n        text: answer\n      }\n    };\n  });\n  return {\n    '@context': 'https://schema.org',\n    '@type': 'FAQPage',\n    mainEntity: mainEntity\n  };\n};\nexport var OrganizationShema = function OrganizationShema() {\n  return {\n    '@context': 'https://schema.org',\n    '@type': 'Organization',\n    url: \"\".concat(SITE_DOMAIN),\n    name: 'ScienceSoft',\n    logo: '/logo.svg',\n    email: \"\".concat(MAIN_CONTACT_EMAIL),\n    address: {\n      '@type': 'PostalAddress',\n      streetAddress: \"\".concat(COMPANY_STREET_ADDRESS),\n      addressLocality: \"\".concat(COMPANY_ADDRESS_LOCALITY),\n      postalCode: \"\".concat(COMPANY_POSTAL_CODE)\n    },\n    telePhone: \"\".concat(INTERNATIONAL)\n  };\n};\nexport var AuthorPageShema = function AuthorPageShema(_ref3) {\n  var name = _ref3.name,\n      id = _ref3.id,\n      seoImages = _ref3.seoImages,\n      socialNetworks = _ref3.socialNetworks,\n      position = _ref3.position;\n  return {\n    '@context': 'https://schema.org/',\n    '@type': 'Person',\n    name: name,\n    url: \"\".concat(SITE_DOMAIN).concat(id),\n    image: seoImages.mb,\n    sameAs: !socialNetworks ? [] : Object.values(socialNetworks),\n    jobTitle: position,\n    worksFor: {\n      '@type': 'Organization',\n      name: \"WTT Solutions\"\n    }\n  };\n};","map":null,"metadata":{},"sourceType":"module"}