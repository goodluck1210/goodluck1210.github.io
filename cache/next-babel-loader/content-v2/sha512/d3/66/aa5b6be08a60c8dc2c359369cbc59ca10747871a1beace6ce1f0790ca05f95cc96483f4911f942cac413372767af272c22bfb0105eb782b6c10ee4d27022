l');\r\n    } else {\r\n      document.body.classList.remove('no-sroll');\r\n    }\r\n  }, [isOpen]);\r\n\r\n  const togglesClass = isOpen ? `${style.closeBtn}` : `${style.openBtn}`;\r\n\r\n  const getClassNames = (route) => {\r\n    return classNames(\r\n      style.headerMenuItem,\r\n      pathname.includes(route) && style.selected\r\n    );\r\n  };\r\n\r\n  const getDropdownClassNames = (links) => {\r\n    return classNames(\r\n      style.headerMenuItem,\r\n      links.some(({ link }) => pathname.includes(link)) &&\r\n        !pathname.includes('blog/') &&\r\n        style.dropdownSelected\r\n    );\r\n  };\r\n\r\n  const handleMenuItem = () => {\r\n    setIsOpen(false);\r\n  };\r\n\r\n  const handleMobileDropdownItem = (e) => {\r\n    const index = e.target.id.slice(-1);\r\n    const dropdown = document.getElementsByClassName(`dropdown${index}`)[0];\r\n    const arrow = document.getElementsByClassName(`arrow${index}`)[0];\r\n    if (\r\n      dropdown.style.visibility === 'hidden' ||\r\n      dropdown.style.visibility === ''\r\n    ) {\r\n      dropdown.style.visibility = 'visible';\r\n      dropdown.style.height = 'auto';\r\n      arrow.style.transform = 'rotate(0deg)';\r\n      arrow.style.filter =\r\n        'invert(43%) sepia(90%) saturate(419%) hue-rotate(88deg) brightness(94%) contrast(98%)';\r\n      return;\r\n    }\r\n    dropdown.style.visibility = 'hidden';\r\n    dropdown.style.height = 0;\r\n    arrow.style.transform = 'rotate(180deg)';\r\n    arrow.style.filter = 'none';\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <Navbar\r\n        light\r\n        expand=\"xl\"\r\n        className={classNames(\r\n          style.Header,\r\n          isTop && style['Header--onTop'],\r\n          isOpen && style['Header--isOpen']\r\n        )}\r\n      >\r\n        <Link href={routing().root}>\r\n          <a className={style.Header__logo}>\r\n            <img\r\n              src=\"/logo.svg\"\r\n              className={style.logoImg}\r\n              alt=\"logo\"\r\n              width=\"40\"\r\n              height=\"40\"\r\n            />\r\n            <span>WTT SOLUTIONS</span>\r\n          </a>\r\n        </Link>\r\n\r\n        <button type=\"button\" onClick={() => changeLanguage('de')}>\r\n          {t('translation:de')}\r\n        </button>\r\n\r\n        <button type=\"button\" onClick={() => changeLanguage('en')}>\r\n          {t('translation:en')}\r\n        </button>\r\n\r\n        <h1>{t('welcome:title', 'Hello there.')}</h1>\r\n        <br/>\r\n        <p>{t('welcome:content.textOne', 'Welcome at our place.')}</p>\r\n\r\n        <NavbarToggler onClick={toggle} className={togglesClass} />\r\n\r\n        <Collapse isOpen={isOpen} navbar className={style.headerCollapse}>\r\n          <Nav navbar className={style.headerNavbar}>\r\n            {menuItems.map(({ name, link, dropdownItems }, index) => {\r\n              return (\r\n                <Fragment key={index}>\r\n                  {dropdownItems ? (\r\n                    <li\r\n                      className={classNames(\r\n                        style.headerMenuItem,\r\n                        style.dropdown\r\n                      )}\r\n                    >\r\n                      <a\r\n                        id={`dropdown${index + 1}`}\r\n                        className={classNames(\r\n                          getDropdownClassNames(dropdownItems)\r\n                        )}\r\n                        onClick={width < 1200 ? handleMobileDropdownItem : null}\r\n                      >\r\n                        {name}\r\n                        <img\r\n                          width={32}\r\n                          height={7}\r\n                          alt=\"arrow\"\r\n                          className={`arrow${index + 1}`}\r\n                          id={`dropdownImg${index + 1}`}\r\n                          src=\"/navigateDown.svg\"\r\n                        />\r\n                      </a>\r\n\r\n                      <div\r\n                        className={classNames(\r\n                          style.dropdownItemWrapper,\r\n                          `dropdown${index + 1}`\r\n                        )}\r\n                      >\r\n                        <div className={style.dropdownItem}>\r\n                          {dropdownItems.map(({ name, link }, index) => (\r\n                            <Link href={link} key={index}>\r\n                              <a\r\n                                onClick={() => handleMenuItem(index + 1)}\r\n                                className={classNames(\r\n                                  pathname.includes(link) &&\r\n                                    !pathname.includes('blog/') &&\r\n                                    style.selected\r\n                                )}\r\n                                key={index}\r\n                              >\r\n                                {name}\r\n                              </a>\r\n                            </Link>\r\n                          ))}\r\n                        </div>\r\n                      </div>\r\n                    </li>\r\n                  ) : (\r\n                    <NavItem\r\n                      onClick={handleMenuItem}\r\n                      className={getClassNames(link)}\r\n                      key={index}\r\n                    >\r\n                      <Link href={link}>\r\n                        <a\r\n                          style={{\r\n                            color: pathname.includes(link)\r\n                              ? '#1ba94c'\r\n                              : 'white',\r\n                          }}\r\n                        >\r\n                          {name}\r\n                        </a>\r\n                      </Link>\r\n                    </NavItem>\r\n                  )}\r\n                </Fragment>\r\n              );\r\n            })}\r\n          </Nav>\r\n        </Collapse>\r\n      </Navbar>\r\n      <style jsx>\r\n        {`\r\n          a {\r\n            transition: all 0.1s;\r\n            text-decoration: none;\r\n            color: #fff;\r\n            font-size: 16px;\r\n            line-height: 1.6;\r\n          }\r\n        `}\r\n      </style>\r\n    </>\r\n  );\r\n}\r\n"]},"metadata":{},"sourceType":"module"}