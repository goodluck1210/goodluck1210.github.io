org',\n    '@type': 'FAQPage',\n    mainEntity: mainEntity\n  };\n};\nexport const OrganizationShema = () => {\n  return {\n    '@context': 'https://schema.org',\n    '@type': 'Organization',\n    url: `${SITE_DOMAIN}`,\n    name: 'ScienceSoft',\n    logo: '/logo.svg',\n    email: `${MAIN_CONTACT_EMAIL}`,\n    address: {\n      '@type': 'PostalAddress',\n      streetAddress: `${COMPANY_STREET_ADDRESS}`,\n      addressLocality: `${COMPANY_ADDRESS_LOCALITY}`,\n      postalCode: `${COMPANY_POSTAL_CODE}`\n    },\n    telePhone: `${INTERNATIONAL}`\n  };\n};\nexport const AuthorPageShema = ({\n  name,\n  id,\n  seoImages,\n  socialNetworks,\n  position\n}) => {\n  return {\n    '@context': 'https://schema.org/',\n    '@type': 'Person',\n    name: name,\n    url: `${SITE_DOMAIN}${id}`,\n    image: seoImages.mb,\n    sameAs: !socialNetworks ? [] : Object.values(socialNetworks),\n    jobTitle: position,\n    worksFor: {\n      '@type': 'Organization',\n      name: \"WTT Solutions\"\n    }\n  };\n};","map":null,"metadata":{},"sourceType":"module"}