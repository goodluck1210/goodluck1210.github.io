Entity = questions.map(function (_ref2) {\n    var question = _ref2.question,\n        answer = _ref2.answer;\n    return {\n      '@type': 'Question',\n      name: question,\n      acceptedAnswer: {\n        '@type': 'Answer',\n        text: answer\n      }\n    };\n  });\n  return {\n    '@context': 'https://schema.org',\n    '@type': 'FAQPage',\n    mainEntity: mainEntity\n  };\n};\n_c = FAQShema;\nexport var OrganizationShema = function OrganizationShema() {\n  return {\n    '@context': 'https://schema.org',\n    '@type': 'Organization',\n    url: \"\".concat(SITE_DOMAIN),\n    name: 'ScienceSoft',\n    logo: '/logo.svg',\n    email: \"\".concat(MAIN_CONTACT_EMAIL),\n    address: {\n      '@type': 'PostalAddress',\n      streetAddress: \"\".concat(COMPANY_STREET_ADDRESS),\n      addressLocality: \"\".concat(COMPANY_ADDRESS_LOCALITY),\n      postalCode: \"\".concat(COMPANY_POSTAL_CODE)\n    },\n    telePhone: \"\".concat(INTERNATIONAL)\n  };\n};\n_c2 = OrganizationShema;\nexport var AuthorPageShema = function AuthorPageShema(_ref3) {\n  var name = _ref3.name,\n      id = _ref3.id,\n      seoImages = _ref3.seoImages,\n      socialNetworks = _ref3.socialNetworks,\n      position = _ref3.position;\n  return {\n    '@context': 'https://schema.org/',\n    '@type': 'Person',\n    name: name,\n    url: \"\".concat(SITE_DOMAIN).concat(id),\n    image: seoImages.mb,\n    sameAs: !socialNetworks ? [] : Object.values(socialNetworks),\n    jobTitle: position,\n    worksFor: {\n      '@type': 'Organization',\n      name: \"WTT Solutions\"\n    }\n  };\n};\n_c3 = AuthorPageShema;\n\nvar _c, _c2, _c3;\n\n$RefreshReg$(_c, \"FAQShema\");\n$RefreshReg$(_c2, \"OrganizationShema\");\n$RefreshReg$(_c3, \"AuthorPageShema\");","map":{"version":3,"sources":["E:/corporate-website/utils/microdata/index.js"],"names":["routing","SITE_DOMAIN","MAIN_CONTACT_EMAIL","INTERNATIONAL","COMPANY_STREET_ADDRESS","COMPANY_ADDRESS_LOCALITY","COMPANY_POSTAL_CODE","breadcrumbShema","blog","category","id"