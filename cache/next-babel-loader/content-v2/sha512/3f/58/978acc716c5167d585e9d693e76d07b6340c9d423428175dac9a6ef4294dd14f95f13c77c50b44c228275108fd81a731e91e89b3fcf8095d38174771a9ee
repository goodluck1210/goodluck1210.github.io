 'next/link';\r\nimport { useRouter } from 'next/router';\r\nimport { Collapse, Navbar, NavbarToggler, Nav, NavItem, DropdownMenu, DropdownItem, ButtonDropdown, DropdownToggle } from 'reactstrap';\r\nimport 'bootstrap/dist/css/bootstrap.min.css';\r\n\r\nimport routing from '../../config/routing';\r\n// import { menuItems } from '../../config/constants';\r\nimport useWindowSize from '../../hooks/useWindowSize';\r\n\r\nimport style from './header.module.scss';\r\nimport classNames from 'classnames';\r\n\r\nimport {\r\n  useTranslation,\r\n  useLanguageQuery,\r\n  LanguageSwitcher,\r\n} from \"next-export-i18n\";\r\n\r\nexport default function Header() {\r\n  const { t } = useTranslation();\r\n  const [query] = useLanguageQuery();\r\n\r\n  console.log('query', query);\r\n  if (query.id){\r\n    delete query.id;\r\n  }\r\n\r\n  const [isOpen, setIsOpen] = useState(false);\r\n  const [isTop, setIsTop] = useState(true);\r\n  const { pathname } = useRouter();\r\n  const { width } = useWindowSize();\r\n  const [dropdownOpen, setOpen] = useState(false);\r\n\r\n  const toggle = () => setIsOpen(!isOpen);\r\n  const handleScroll = () => {\r\n    if (window.scrollY > 11) setIsTop(false);\r\n    else setIsTop(true);\r\n  };\r\n\r\n  useEffect(() => {\r\n    document.addEventListener('scroll', handleScroll);\r\n\r\n    return () => {\r\n      document.removeEventListener('scroll', handleScroll);\r\n    };\r\n  }, []);\r\n\r\n  useEffect(() => {\r\n    if (isOpen) {\r\n      document.body.classList.add('no-sroll');\r\n    } else {\r\n      document.body.classList.remove('no-sroll');\r\n    }\r\n  }, [isOpen]);\r\n\r\n  const togglesClass = isOpen ? `${style.closeBtn}` : `${style.openBtn}`;\r\n\r\n  const getClassNames = (route) => {\r\n    return classNames(\r\n      style.headerMenuItem,\r\n      pathname.includes(route) && style.selected\r\n    );\r\n  };\r\n\r\n  const getDropdownClassNames = (links) => {\r\n    return classNames(\r\n      style.headerMenuItem,\r\n      links.some(({ link }) => pathname.includes(link)) &&\r\n        !pathname.includes('blog/') &&\r\n        style.dropdownSelected\r\n    );\r\n  };\r\n\r\n  const handleMenuItem = () => {\r\n    setIsOpen(false);\r\n  };\r\n\r\n  const handleMobileDropdownItem = (e) => {\r\n    const index = e.target.id.slice(-1);\r\n    const dropdown = document.getElementsByClassName(`dropdown${index}`)[0];\r\n    const arrow = document.getElementsByClassName(`arrow${index}`)[0];\r\n    if (\r\n      dropdown.style.visibility === 'hidden' ||\r\n      dropdown.style.visibility === ''\r\n    ) {\r\n      dropdown.style.visibility = 'visible';\r\n      dropdown.style.height = 'auto';\r\n      arrow.style.transform = 'rotate(0deg)';\r\n      arrow.style.filter =\r\n        'invert(43%) sepia(90%) saturate(419%) hue-rotate(88deg) brightness(94%) contrast(98%)';\r\n      return;\r\n    }\r\n    dropdown.style.visibility = 'hidden';\r\n    dropdown.style.height = 0;\r\n    arrow.style.transform = 'rotate(180deg)';\r\n    arrow.style.filter = 'none';\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <Navbar\r\n        light\r\n        expand=\"xl\"\r\n        className={classNames(\r\n          style.Header,\r\n          isTop && style['Header--onTop'],\r\n          isOpen && style['Header--isOpen']\r\n        )}\r\n      >\r\n        <Link href={{ pathname: routing().root, query: query }} >\r\n          <a className={style.Header__logo}>\r\n            <img\r\n              src=\"/logo.svg\"\r\n              className={style.logoImg}\r\n              alt=\"logo\"\r\n              width=\"40\"\r\n              height=\"40\"\r\n            />\r\n            <span>WTT SOLUTIONS</span>\r\n          </a>\r\n        </Link>\r\n        <NavbarToggler onClick={toggle} className={togglesClass} />\r\n        <Collapse isOpen={isOpen} navbar className={style.headerCollapse}>\r\n          <Nav navbar className={style.headerNavbar}>\r\n            {t('menuItems').map(({ name, link, dropdownItems }, index) => {\r\n              return (\r\n                <Fragment key={index}>\r\n                  {dropdownItems ? (\r\n                    <li\r\n                      className={classNames(\r\n                        style.headerMenuItem,\r\n                        style.dropdown\r\n                      )}\r\n                    >\r\n                      <a\r\n                        id={`dropdown${index + 1}`}\r\n                        className={classNames(\r\n                          getDropdownClassNames(dropdownItems)\r\n                        )}\r\n                        onClick={width < 1200 ? handleMobileDropdownItem : null}\r\n                      >\r\n                        {name}\r\n                        <img\r\n                          width={32}\r\n                          height={7}\r\n                          alt=\"arrow\"\r\n                          className={`arrow${index + 1}`}\r\n                          id={`dropdownImg${index + 1}`}\r\n                          src=\"/navigateDown.svg\"\r\n                        />\r\n                      </a>\r\n\r\n                      <div\r\n                        className={classNames(\r\n                          style.dropdownItemWrapper,\r\n                          `dropdown${index + 1}`\r\n                        )}\r\n                      >\r\n                        <div className={style.dropdownItem}>\r\n                          {dropdownItems.map(({ name, link }, index) => (\r\n                            <Link href={{ pathname: link, query: query }} key={index}>\r\n                              <a\r\n                                onClick={() => handleMenuItem(index + 1)}\r\n                                className={classNames(\r\n                                  pathname.includes(link) &&\r\n                                    !pathname.includes('blog/') &&\r\n                                    style.selected\r\n                                )}\r\n                                key={index}\r\n                              >\r\n                                {name}\r\n                              </a>\r\n                            </Link>\r\n                          ))}\r\n                        </div>\r\n                      </div>\r\n                    </li>\r\n                  ) : (\r\n                    <NavItem\r\n                      onClick={handleMenuItem}\r\n                      className={getClassNames(link)}\r\n                      key={index}\r\n                    >\r\n                      <Link href={{ pathname: link, query: query }} >\r\n                        <a\r\n                          style={{\r\n                            color: pathname.includes(link)\r\n                              ? '#1ba94c'\r\n                              : 'white',\r\n                          }}\r\n                        >\r\n                          {name}\r\n                        </a>\r\n                      </Link>\r\n                    </NavItem>\r\n                  )}\r\n                </Fragment>\r\n              );\r\n            })}\r\n          </Nav>\r\n        </Collapse>\r\n      </Navbar>\r\n      <div className=\"language-switcher\">\r\n          <ButtonDropdown className=\"language-dropdown\" toggle={() => { setOpen(!dropdownOpen) }} isOpen={dropdownOpen}>\r\n              <DropdownToggle caret>\r\n                  <img alt=\"vector\" src={'/images/lang/Vector.png'}/>\r\n                  <img alt=\"arrow\" src=\"/navigateDown.svg\" id=\"langDropdownImg\"></img>\r\n              </DropdownToggle>\r\n              <DropdownMenu>\r\n                  <DropdownItem>\r\n                    <LanguageSwitcher lang=\"en\">\r\n                      <img width={32} height={21.33} src={'/images/lang/en.jpg'}/>\r\n                    </LanguageSwitcher>\r\n                  </DropdownItem>\r\n                  <DropdownItem>   \r\n          \t\t\t\t\t<LanguageSwitcher lang=\"de\">\r\n                      <img wid