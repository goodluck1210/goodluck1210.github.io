{"ast":null,"code":"import { customBtn } from \"../../../config/constants\";\nimport styles from \"./customBtn.module.scss\";\nimport classNames from \"classnames\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\nimport { Fragment as _Fragment } from \"react/jsx-runtime\";\nexport default function CustomBtn(props) {\n  const {\n    PRIMARY,\n    OUTLINE,\n    LARGE,\n    SMALL\n  } = customBtn;\n  const {\n    type = PRIMARY,\n    btnType,\n    size = LARGE,\n    title,\n    onClick,\n    buttonStyle,\n    disabled = false,\n    classes,\n    link,\n    targetBlank = false\n  } = props;\n\n  const generateBtnType = () => {\n    switch (type) {\n      case PRIMARY:\n        return styles.customBtn;\n\n      case OUTLINE:\n        return styles.outlineBtn;\n    }\n  };\n\n  const generateBtnSize = () => {\n    switch (size) {\n      case LARGE:\n        return styles.largeBtn;\n\n      case SMALL:\n        return styles.smallBtn;\n    }\n  };\n\n  return /*#__PURE__*/_jsx(_Fragment, {\n    children: link ? /*#__PURE__*/_jsx(\"a\", {\n      href: link,\n      onClick: onClick,\n      target: !targetBlank ? \"_self\" : \"_blank\",\n      className: classNames(styles.Btn, classes, generateBtnType(), generateBtnSize()),\n      children: title\n    }) : /*#__PURE__*/_jsx(\"button\", {\n      onClick: onClick,\n      type: btnType,\n      style: buttonStyle,\n      className: classNames(styles.Btn, classes, generateBtnType(), generateBtnSize(), disabled && styles.disabledBtn),\n      disabled: disabled,\n      children: title\n    })\n  });\n}","map":null,"metadata":{},"sourceType":"module"}