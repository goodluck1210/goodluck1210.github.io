{"ast":null,"code":"import { clientsAboutUsScroll } from '../../../config/constants'; //add event listener to sidebar items on case pages\n\nexport const caseSidebarEventListener = (elementsArr, scrollFnDesk, scrollFnTablet) => {\n  Array.from(elementsArr).forEach(el => {\n    el.addEventListener('click', () => {\n      if (window.innerWidth < 1200) {\n        scrollFnTablet(Number(el.parentElement.value));\n      } else {\n        scrollFnDesk(Number(el.parentElement.value));\n      }\n    });\n  });\n}; //event listener to buttons \"Clients about us\" on pages\n\nexport const clientsAboutUsBtnEventListeners = scrollFn => {\n  const values = Object.values(clientsAboutUsScroll);\n  values.forEach(({\n    buttonScrollID,\n    slide\n  }) => {\n    const button = document.getElementById(buttonScrollID);\n\n    if (button && window.innerWidth > 1199) {\n      button.addEventListener('click', () => {\n        scrollFn(slide);\n      });\n    }\n  });\n};","map":null,"metadata":{},"sourceType":"module"}