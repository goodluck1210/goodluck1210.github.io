{"ast":null,"code":"import React, { useState } from 'react';\nimport styles from './Questions.module.scss';\nimport { Collapse } from 'reactstrap';\nimport { Fragment as _Fragment } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\nimport { jsxs as _jsxs } from \"react/jsx-runtime\";\n\nconst Questions = ({\n  data\n}) => {\n  const {\n    questionsList,\n    faqInTableOfContents,\n    subtitles,\n    isNoFaq,\n    id\n  } = data;\n  if (isNoFaq) return /*#__PURE__*/_jsx(_Fragment, {});\n  return /*#__PURE__*/_jsx(_Fragment, {\n    children: /*#__PURE__*/_jsx(\"div\", {\n      className: styles.wrapper,\n      id: faqInTableOfContents ? `sub${subtitles[subtitles.length - 1].id}` : null,\n      children: /*#__PURE__*/_jsxs(\"div\", {\n        className: styles.content,\n        children: [/*#__PURE__*/_jsx(\"h3\", {\n          className: styles.mainTitle,\n          children: 'Frequently Asked Questions'.toUpperCase()\n        }), /*#__PURE__*/_jsx(\"div\", {\n          className: styles.block,\n          children: questionsList.map((item, i) => {\n            return /*#__PURE__*/_jsx(Answer, {\n              question: item.question,\n              answer: item.answer,\n              id: id\n            }, i);\n          })\n        })]\n      })\n    })\n  });\n};\n\nconst Answer = ({\n  question,\n  answer\n}) => {\n  const {\n    0: isOpen,\n    1: setIsOpen\n  } = useState(false);\n\n  const toggleCollapse = () => setIsOpen(!isOpen);\n\n  return /*#__PURE__*/_jsxs(\"div\", {\n    className: styles.item,\n    children: [/*#__PURE__*/_jsxs(\"div\", {\n      className: styles.questionBlock,\n      onClick: toggleCollapse,\n      children: [/*#__PURE__*/_jsx(\"div\", {\n        className: styles.icon,\n        children: /*#__PURE__*/_jsx(\"span\", {\n          children: !isOpen ? '+' : '-'\n        })\n      }), /*#__PURE__*/_jsx(\"p\", {\n        className: styles.question,\n        children: question\n      })]\n    }), /*#__PURE__*/_jsx(Collapse, {\n      isOpen: isOpen,\n      children: /*#__PURE__*/_jsx(\"div\", {\n        className: styles.answer,\n        dangerouslySetInnerHTML: {\n          __html: answer\n        }\n      })\n    })]\n  });\n};\n\nexport default Questions;","map":null,"metadata":{},"sourceType":"module"}